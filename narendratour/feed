			<script>
const carousely = document.querySelector(".carousely"),
firstImgy = carousely.querySelectorAll("img")[0],
arrowIconsy = document.querySelectorAll(".wrappery i");

let isDragStarty = false, isDraggingy = false, prevPageXy, prevScrollLefty, positionDiffy;

const showHideIconsy = () => {
    // showing and hiding prev/next icon according to carousely scroll left value
    let scrollWidth = carousely.scrollWidth - carousely.clientWidth; // getting max scrollable width
    arrowIconsy[0].style.display = carousely.scrollLeft == 0 ? "none" : "block";
    arrowIconsy[1].style.display = carousely.scrollLeft == scrollWidth ? "none" : "block";
}

arrowIconsy.forEach(icon => {
    icon.addEventListener("click", () => {
        let firstImgyWidth = firstImgy.clientWidth + 14; // getting first img width & adding 14 margin value
        // if clicked icon is left, reduce width value from the carousely scroll left else add to it
        carousely.scrollLeft += icon.id == "left" ? -firstImgyWidth : firstImgyWidth;
        setTimeout(() => showHideIconsy(), 60); // calling showHideIcons after 60ms
    });
});

const autoSlidey = () => {
    // if there is no image left to scroll then return from here
    if(carousely.scrollLeft - (carousely.scrollWidth - carousely.clientWidth) > -1 || carousely.scrollLeft <= 0) return;

    positionDiffy = Math.abs(positionDiffy); // making positionDiffy value to positive
    let firstImgyWidth = firstImgy.clientWidth + 14;
    // getting difference value that needs to add or reduce from carousely left to take middle img center
    let valDifference = firstImgyWidth - positionDiffy;

    if(carousely.scrollLeft > prevScrollLefty) { // if user is scrolling to the right
        return carousely.scrollLeft += positionDiffy > firstImgyWidth / 3 ? valDifference : -positionDiffy;
    }
    // if user is scrolling to the left
    carousely.scrollLeft -= positionDiffy > firstImgyWidth / 3 ? valDifference : -positionDiffy;
}

const dragStarty = (e) => {
    // updatating global variables value on mouse down event
    isDragStarty = true;
    prevPageXy = e.pageX || e.touches[0].pageX;
    prevScrollLefty = carousely.scrollLeft;
}

const draggingy = (e) => {
    // scrolling images/carousely to left according to mouse pointer
    if(!isDragStarty) return;
    e.preventDefault();
    isDraggingy = true;
    carousely.classList.add("dragging");
    positionDiffy = (e.pageX || e.touches[0].pageX) - prevPageXy;
    carousely.scrollLeft = prevScrollLefty - positionDiffy;
    showHideIconsy();
}

const dragStopy = () => {
    isDragStarty = false;
    carousely.classList.remove("dragging");

    if(!isDraggingy) return;
    isDraggingy = false;
    autoSlidey();
}

carousely.addEventListener("mousedown", dragStarty);
carousely.addEventListener("touchstart", dragStarty);

document.addEventListener("mousemove", draggingy);
carousely.addEventListener("touchmove", draggingy);

document.addEventListener("mouseup", dragStopy);
carousely.addEventListener("touchend", dragStopy);


const carouselx = document.querySelector(".carouselx"),
firstImg = carouselx.querySelectorAll("img")[0],
arrowIcons = document.querySelectorAll(".wrapperx i");

let isDragStart = false, isDragging = false, prevPageX, prevScrollLeft, positionDiff;

const showHideIcons = () => {
    // showing and hiding prev/next icon according to carouselx scroll left value
    let scrollWidth = carouselx.scrollWidth - carouselx.clientWidth; // getting max scrollable width
    arrowIcons[0].style.display = carouselx.scrollLeft == 0 ? "none" : "block";
    arrowIcons[1].style.display = carouselx.scrollLeft == scrollWidth ? "none" : "block";
}

arrowIcons.forEach(icon => {
    icon.addEventListener("click", () => {
        let firstImgWidth = firstImg.clientWidth + 14; // getting first img width & adding 14 margin value
        // if clicked icon is left, reduce width value from the carouselx scroll left else add to it
        carouselx.scrollLeft += icon.id == "left" ? -firstImgWidth : firstImgWidth;
        setTimeout(() => showHideIcons(), 60); // calling showHideIcons after 60ms
    });
});

const autoSlide = () => {
    // if there is no image left to scroll then return from here
    if(carouselx.scrollLeft - (carouselx.scrollWidth - carouselx.clientWidth) > -1 || carouselx.scrollLeft <= 0) return;

    positionDiff = Math.abs(positionDiff); // making positionDiff value to positive
    let firstImgWidth = firstImg.clientWidth + 14;
    // getting difference value that needs to add or reduce from carouselx left to take middle img center
    let valDifference = firstImgWidth - positionDiff;

    if(carouselx.scrollLeft > prevScrollLeft) { // if user is scrolling to the right
        return carouselx.scrollLeft += positionDiff > firstImgWidth / 3 ? valDifference : -positionDiff;
    }
    // if user is scrolling to the left
    carouselx.scrollLeft -= positionDiff > firstImgWidth / 3 ? valDifference : -positionDiff;
}

const dragStart = (e) => {
    // updatating global variables value on mouse down event
    isDragStart = true;
    prevPageX = e.pageX || e.touches[0].pageX;
    prevScrollLeft = carouselx.scrollLeft;
}

const dragging = (e) => {
    // scrolling images/carouselx to left according to mouse pointer
    if(!isDragStart) return;
    e.preventDefault();
    isDragging = true;
    carouselx.classList.add("dragging");
    positionDiff = (e.pageX || e.touches[0].pageX) - prevPageX;
    carouselx.scrollLeft = prevScrollLeft - positionDiff;
    showHideIcons();
}

const dragStop = () => {
    isDragStart = false;
    carouselx.classList.remove("dragging");

    if(!isDragging) return;
    isDragging = false;
    autoSlide();
}

carouselx.addEventListener("mousedown", dragStart);
carouselx.addEventListener("touchstart", dragStart);

document.addEventListener("mousemove", dragging);
carouselx.addEventListener("touchmove", dragging);

document.addEventListener("mouseup", dragStop);
carouselx.addEventListener("touchend", dragStop);
 

