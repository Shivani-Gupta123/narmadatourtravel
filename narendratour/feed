	e(document).ready(function() {
$("ul.inBtp li.expaBox2").click(function(e) {
    e.preventDefault();
    $(this).text() == "See More" ? $(this).text("See Less") : $(this).text("See More");
    $("ul.inBtp li.click_Box").slideToggle(200);
  });
$("li.expaBoxx3").click(function(e) {
    e.preventDefault();
    $(this).text() == "See More" ? $(this).text("See Less") : $(this).text("See More");
    $("ul.highlight-this-tour li.click_Box").slideToggle(200);
  });
  $("ul.inBtpx li.expaBox2").click(function(e) {
    e.preventDefault();
    $(this).text() == "See More" ? $(this).text("See Less") : $(this).text("See More");
    $("ul.inBtpx li.click_Box").slideToggle(200);
  });
		$(".click_quess").click(function() {
			return e(this).hasClass("active") ? (e(".click_quess").removeClass("active"), e(".ans_secs").slideUp("1000")) : (e(".click_quess").removeClass("active"), e(".ans_secs").slideUp("1000"), e(this).addClass("active"), e(this).next().slideDown("1000")), !1
		}), e(".expaBox").click(function() {
			return e(".tabLeft .ans_secs").hasClass("active") ? (e(".tabLeft .ans_secs").removeClass("active"), e(".tabLeft .ans_secs").slideUp("1000")) : (e(".tabLeft .ans_secs").addClass("active"), e(".tabLeft .ans_secs").slideDown("1000")), !1
		}), e(".expaBoxs").click(function() {
			e(".ans_secss").slideToggle("1000")
		}), e(".expaBox3").click(function() {
			e(".tabLeftb4 .ans_secs").slideToggle("1000")
		}), e(".expaBox4").click(function() {
			e(".tab5Box .ans_secs").slideToggle("1000")
		});
	});
});

$('button.btn3').click(function() {
			 
        $(this).parent().find(".readmores").slideToggle(400);
        $(this).text($(this).text() == 'View Less [-]' ? 'View More [+]' : 'View Less [-]');
        return false;
      });

jQuery(window).scroll(function() {
	var sticky = jQuery('.stiky-box'),
		scroll = jQuery(window).scrollTop();

	if (scroll >= 800) sticky.addClass('fixed');
	else sticky.removeClass('fixed');
});

</script>
